
! terminal colors ------------------------------------------------------------
#include "/home/dmi3y/.colours/dark"

! applications ---------------------------------------------------------------

Xcursor*theme:  Vanilla-DMZ-AA
Xcursor.size:   22

Emacs.font:  Terminus 10
Emacs.fullscreen: maximized


urxvt*font:      -*-proggyclean-*-*-*-*-*-*-*-*-*-*-*-*
urxvt*boldFont:  -*-proggyclean-*-*-*-*-*-*-*-*-*-*-*-*
urxvt*geometry:           160x50
urxvt*internalBorder:     5
urxvt*fading:             20
urxvt*shading:            80
urxvt*tintColor:          #222222
urxvt*inheritPixmap:      false
urxvt*depth:              32
urxvt*saveLines:          32767
urxvt*visualBell:         true
urxvt*scrollTtyKeypress:  true
urxvt*scrollWithBuffer:   false
urxvt*scrollTtyOutput:    false
urxvt*scrollBar:          false
urxvt*scrollstyle:        plain
urxvt*scrollBar_right:    true
urxvt*scrollColor:        #777777
urxvt*cursorColor:        #ffcc00
urxvt*loginShell:         true
urxvt*termName:           rxvt
urxvt*cutchars:           "()*,<>[]{}|'"
urxvt*print-pipe:         cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*secondaryScroll:    true
urxvt*mapAlert:           true
urxvt*utmpInhibit:        true
urxvt*perl-lib:           /usr/lib/urxvt/perl/
urxvt*perl-ext-common:    default,matcher,fullscreen
urxvt*keysym.F11:         perl:fullscreen:switch
urxvt*urlLauncher:        /usr/bin/chromium
urxvt*matcher.button:     1
urxvt*background:         rgba:0000/0000/0000/dddd
